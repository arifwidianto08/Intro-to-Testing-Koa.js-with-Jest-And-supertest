{
	"name": "testing-koajs-with-jest-and-superagent",
	"version": "1.0.0",
	"main": "src/server.js",
	"license": "MIT",
	"scripts": {
		"test": "jest --coverage --runInBand",
		"dev": "nodemon"
	},
	"dependencies": {
		"@koa/router": "^10.1.0",
		"koa": "^2.13.1",
		"koa-body": "^4.2.0"
	},
	"devDependencies": {
		"jest": "^27.0.6",
		"nodemon": "^2.0.12",
		"supertest": "^6.1.6"
	},
	"jest": {
		"testEnvironment": "node",
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		],
		"coverageReporters": [
			"text",
			"text-summary"
		]
	}
}
